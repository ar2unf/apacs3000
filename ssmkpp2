			Else
				'msgbox "Новое сообщение зарегестрированно"
				msgbox "Обнаружен алкоголь у "+strNameKPP2s3+" на объекте "+strInitKPP2s3
			End If
		End If
	end if
'обнуление переменных
	strInitKPP2s3 = ""
	strNameKPP2s3 = ""
	dwCardKPP2s3  = 0
	set ObjKPP2s3 = Nothing
end if

